  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithbaylor/ynab-reporting/issues"
  },
  "homepage": "https://github.com/ksmithbaylor/ynab-reporting#readme",
  "dependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "electron": "^1.4.13",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.5.4",
    "elm-webpack-loader": "^4.1.1",
    "npm-run-all": "^4.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
