{
  "name": "ynab-reporting",
  "version": "1.0.0",
  "description": "An electron-based reporting tool for YNAB data, written in Elm",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev-server dev-electron",
    "compile": "webpack",
    "electron": "npm run compile && electron .",
    "dev-electron": "HOT=true electron .",
    "dev-server": "webpack-dev-server --hot --inline --port 3000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksmithbaylor/ynab-reporting.git"
  },
